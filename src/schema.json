{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://opawg.org/user-agents/schema.json",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "user_agents"
    ],
    "properties": {
      "user_agents": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "bot": {
        "type": "boolean"
      },
      "app": {
        "type": "string"
      },
      "device": {
        "type": "string"
      },
      "os": {
        "type": "string"
      },
      "examples": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "description": {
        "type": "string"
      },
      "info_url": {
        "type": "string"
      },
      "svg": {
        "type": "string"
      },
      "developer_notes": {
        "type": "string"
      }
    }
  }
}
